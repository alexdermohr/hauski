{
  "problemMatcher": [
    {
      "owner": "rustc",
      "pattern": [
        {
          "regexp": "^(error|warning|note|help)(\\[(E\\d{4}|[\\w\\-]+)\\])?:\\s+(.*)$",
          "severity": 1,
          "code": 3,
          "message": 4
        },
        {
          "regexp": "^\\s*-->\\s+([^:]+):(\\d+):(\\d+)$",
          "file": 1,
          "line": 2,
          "column": 3,
          "message": 0
        },
        {
          "regexp": "^(\\s*\\|)$",
          "message": 1
        },
        {
          "regexp": "^\\s*\\|\\s*(.*)$",
          "loop": true,
          "message": 1
        }
      ]
    }
  ]
}
