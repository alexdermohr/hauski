{
  "problemMatcher": [
    {
      "owner": "rustc-error",
      "pattern": [
        {
          "regexp": "^(error)(\\[[^\\]]+\\])?:\\s+(.*)$",
          "message": 3,
          "severity": 1
        },
        {
          "regexp": "^\\s*-->\\s+([^:]+):(\\d+):(\\d+)$",
          "file": 1,
          "line": 2,
          "column": 3
        },
        {
          "regexp": "^\\s*[|].*$",
          "loop": true,
          "message": 3
        }
      ]
    },
    {
      "owner": "rustc-warning",
      "pattern": [
        {
          "regexp": "^(warning)(\\[[^\\]]+\\])?:\\s+(.*)$",
          "message": 3,
          "severity": 2
        },
        {
          "regexp": "^\\s*-->\\s+([^:]+):(\\d+):(\\d+)$",
          "file": 1,
          "line": 2,
          "column": 3
        },
          "regexp": "^\\s*[|].*$",
          "loop": true,
          "message": 3
  
        }
      ]
    }
  ]
}
